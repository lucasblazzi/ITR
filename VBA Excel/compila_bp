Sub compila_bp()
'
' compila_bp Macro
'
' Atalho do teclado: Ctrl+w
'
    Sheets("MGLU3_2017_BPA").Select
    Range("C5:D265").Select
    Selection.Copy
    Sheets("MGLU3_2019_BPA").Select
    Range("E5").Select
    ActiveSheet.Paste
    Range("C69").Select
    Sheets("MGLU3_2015_BPA").Select
    Range("C5:D261").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("MGLU3_2019_BPA").Select
    Range("G5").Select
    ActiveSheet.Paste
    Range("D72").Select
End Sub
